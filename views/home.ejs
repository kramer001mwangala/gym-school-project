      <div class="loader"> <!-- Loader styles go here --> </div>
      <div class='prices'>
        <!-- Content for the prices section goes here -->
      </div>
    
      <script>
        // Add your JavaScript code here
        document.addEventListener('DOMContentLoaded', function () {
          setTimeout(function () {
            document.getElementById('letterH').classList.add('text-animate-hover');
            // Add similar lines for other letters if needed
          }, 4000);
        });
      </script>
